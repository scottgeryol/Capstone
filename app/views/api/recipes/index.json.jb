@recipes.map do |recipe|
  {
    id: recipe.id,
    name: recipe.name,
    chef: recipe.chef,
    description: recipe.description,
    directions: recipe.directions,
    prep_time: recipe.prep_time,
    notes: recipe.notes,
    user_id: recipe.user_id,
    image_url: recipe.image_url,
    ingredients: recipe.ingredients,
  }
end
